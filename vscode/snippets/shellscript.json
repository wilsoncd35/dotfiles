{
  "shebang bash": {
    "prefix": "shebang",
    "body": ["#!/usr/bin/env bash"]
  },
  "shebang zsh": {
    "prefix": "shebangz",
    "body": ["#!/usr/bin/env bash"]
  },
  "now utc": {
    "prefix": "now utc",
    "body": ["now=\"$(date -u \"+%Y-%m-%d-%H%M%S\")\""]
  },
  "now local": {
    "prefix": "now",
    "body": ["now=\"$(date \"+%Y-%m-%d-%H%M%S\")\""]
  },
  "fail is zero or exit 1": {
    "prefix": "fail01",
    "body": ["[ \"\\$fail\" -eq 0 ] || exit 1"]
  },
  "test command available": {
    "prefix": "command available",
    "body": [
      "command -v $1 > /dev/null 2>&1",
      "$0"
    ]
  }
}
